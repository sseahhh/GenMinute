# Google API Keys
GOOGLE_API_KEY=your_google_api_key_here
GCP_API_KEY=your_gcp_api_key_here
GOOGLE_APPLICATION_CREDENTIALS=path_to_your_service_account_json

# Firebase Configuration (Client-side)
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id
FIREBASE_MEASUREMENT_ID=your_measurement_id

# Flask Configuration
FLASK_SECRET_KEY=generate_with_secrets_token_hex_32
ADMIN_EMAILS=admin@example.com,admin2@example.com

# LangSmith (Optional)
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your_langsmith_key
LANGSMITH_PROJECT=your_project_name

# Other API Keys
OPENAI_API_KEY=your_openai_key
SERPAPI_API_KEY=your_serpapi_key
LLAMA_CLOUD_API_KEY=your_llama_key
HUGGINGFACEHUB_API_TOKEN=your_huggingface_token
UPSTAGE_API_KEY=your_upstage_key
PINECONE_API_KEY=your_pinecone_key
COHERE_API_KEY=your_cohere_key
TAVILY_API_KEY=your_tavily_key

# Naver Cloud
NAVER_CLIENT_ID=your_naver_client_id
NAVER_CLIENT_SECRET=your_naver_client_secret
CLOVASPEECH_API=your_clova_api_key
CLOVASPEECH_INVOKE_URL=your_clova_invoke_url

# Google Cloud Storage
GCS_BUCKET_NAME=your_bucket_name

# Pyannote
PYANNOTE_TOKEN=your_pyannote_token
